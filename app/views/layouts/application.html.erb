<!DOCTYPE html>
<html>
<head>
  <title>RailsTest</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body style="background: #EEEEEE; text-align: center">

<% if notice %>
  <div class="notice"><%= notice %></div>
<% end %>
<% if alert %>
  <div class="notice"><%= alert %></div>
<% end %>

<%= yield %>

</body>
</html>
